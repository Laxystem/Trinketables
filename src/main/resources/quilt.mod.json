{
	"schema_version": 1,
	"quilt_loader": {
		"group": "quest.laxla",
		"id": "trinketables",
		"version": "${version}",
		"metadata": {
			"name": "Trinketables",
			"description": "All items deserve to be trinkets!",
			"contributors": {
				"Laxystem": "Author"
			},
			"contact": {
				"homepage": "https://trinketables.laxla.quest/",
				"issues": "https://github.com/LaylaMeower/Trinketable/issues",
				"sources": "https://github.com/LaylaMeower/Trinketable"
			},
			"icon": "assets/trinketables/icon.png",
			"license": "MPL-2.0"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "quest.laxla.trinketables.Trinketables"
			},
			"client_init": {
				"adapter": "kotlin",
				"value": "quest.laxla.trinketables.client.TrinketablesClient"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.2"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "qkl",
				"versions": ">=2.1.0"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			},
			{
				"id": "trinkets",
				"versions": ">=3.7.0"
			},
			{
				"id": "modmenu",
				"versions": "*",
				"optional": true,
				"environment": "client"
			}
		]
	},
	"mixin": "trinketables.mixin.json",
	"modmenu": {
		"links": {
			"modmenu.discord": "https://discord.gg/z9N5n5ztbQ"
		},
		"update_checker": true
	}
}
